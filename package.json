{
  "name": "reselect",
  "version": "5.0.0",
  "description": "Selectors for Redux.",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "jsnext:main": "es/index.js",
  "module": "es/index.js",
  "files": [
    "lib",
    "src",
    "dist",
    "es"
  ],
  "bugs": {
    "url": "https://github.com/reduxjs/reselect/issues"
  },
  "scripts": {
    "compile:commonjs": "tsc --declaration",
    "compile:umdmin": "terser dist/reselect.js -mt -o dist/reselect.min.js",
    "compile:umd": "better-npm-run compile:umd",
    "compile:es": "babel -d es/ src/",
    "compile": "npm run compile:commonjs && npm run compile:umd && npm run compile:umdmin && npm run compile:es",
    "prettier": "prettier --write src/index.ts test/index.spec.js",
    "test": "jest --coverage",
    "typecheck": "tsc --noEmit",
    "typescript-test": "typings-tester --dir typescript_test"
  },
  "keywords": [
    "selectors",
    "react",
    "redux"
  ],
  "authors": [
    "Lee Bannard",
    "Robert Binna",
    "Martijn Faassen",
    "Philip Spitzlinger"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/reduxjs/reselect.git"
  },
  "license": "MIT",
  "devDependencies": {
    "jest": "^24.9.0",
    "lodash.memoize": "^4.1.2",
    "prettier": "^1.18.2",
    "terser": "^4.2.0",
    "ts-jest": "^24.0.2",
    "typescript": "^3.5.3",
    "typings-tester": "^0.3.2"
  },
  "sideEffects": false
}
